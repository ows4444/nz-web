{
	"singleQuote": true,
	"semi": true,
	"printWidth": 120,
	"useTabs": true, 
	"trailingComma": "none",
	"endOfLine": "auto",
	"arrowParens": "always",
	"quoteProps": "as-needed",
	"bracketSpacing": true,
	"jsxBracketSameLine": false,
	"htmlWhitespaceSensitivity": "ignore",
	"importOrder": [
		"^(react/(.*)$)|^(react$)|^(react-(.*)$)",
		"^(next/(.*)$)|^(next$)|^(next-(.*)$)",
		"^styled-components",
		"",
		"^@tanstack/(.*)$",
		"^@reduxjs/(.*)$",
		"",
		"^@app/(.*)$",
		"^@components/(.*)$",
		"^@forms/(.*)$",
		"^@providers/(.*)$",
		"^@styles/(.*)$",
		"^@store/(.*)$",
		"^@query/(.*)$",
		"",
		"^[./]"
	],
	"plugins": [
		"@ianvs/prettier-plugin-sort-imports"
	]
}